{
    "name": "kineticode",
    "displayName": "Kineticode: Air Gesture Control",
    "description": "Control VS Code with hand gestures and posture monitoring.",
    "version": "0.1.1",
    "publisher": "Kineticode1",
    "license": "MIT",
    "qna": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/lcscaletty/psychic-octo-palm-tree"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other",
        "Machine Learning"
    ],
    "keywords": [
        "gesture",
        "posture",
        "camera",
        "mediapipe",
        "automation"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "air-gesture.selectMode",
                "title": "Kineticode: Open Control Menu"
            },
            {
                "command": "air-gesture.stop",
                "title": "Kineticode: Stop Detection"
            }
        ],
        "configuration": {
            "title": "Kineticode Control",
            "properties": {
                "airGesture.debugWindow": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show/hide the camera preview and tracking visualizers."
                }
            }
        }
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.80.0",
        "@types/node": "^16.11.7",
        "typescript": "^4.5.2"
    }
}